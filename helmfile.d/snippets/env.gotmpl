{{ printf "%s-%s" .cluster.provider .cluster.name }}:
  missingFileHandler: warn
  values:
    - {{- . | toYaml | nindent 6 }}
    - ../env/env/default.yaml
    - ../env/env/secrets.yaml.dec
    - ../env/env/{{ .cluster.provider }}/default.yaml
    - ../env/env/{{ .cluster.provider }}/secrets.yaml.dec
    - ../env/env/{{ .cluster.provider }}/{{ .cluster.name }}.yaml
    - ../env/env/{{ .cluster.provider }}/secrets.{{ .cluster.name }}.yaml.dec
    - ../core.yaml
