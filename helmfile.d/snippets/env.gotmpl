{{ printf "%s-%s" .provider .stage }}:
  values:
    - stage: {{ .stage }}
      provider: {{ .provider }}
    - ../values/_env/teams.yaml
    - ../values/_env/default.yaml
    - ../values/_env/{{ .stage }}.yaml
    - ../values/_env/{{ .provider }}/teams.yaml
    - ../values/_env/{{ .provider }}/default.yaml
    - ../values/_env/{{ .provider }}/{{ .stage }}.yaml
    - ../core.yaml
