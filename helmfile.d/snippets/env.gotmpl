{{ printf "%s-%s" .cluster.provider .cluster.name }}:
  missingFileHandler: Warn
  values:
    - {{- . | toYaml | nindent 6 }}
    - ../env/env/default.yaml.dec
    - ../env/env/{{ .cluster.provider }}/default.yaml.dec
    - ../env/env/{{ .cluster.provider }}/{{ .cluster.name }}.yaml.dec
    - ../core.yaml
