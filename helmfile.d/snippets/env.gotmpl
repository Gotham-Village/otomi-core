{{ printf "%s-%s" .cluster.provider .cluster.name }}:
  values:
    - clouds: {{- .clouds | toYaml | nindent 8 }}
    - cluster: {{- .cluster | toYaml | nindent 8 }}
    - ../env/teams.yaml
    - ../env/default.yaml
    - ../env/{{ .cluster.name }}.yaml
    - ../env/{{ .cluster.provider }}/teams.yaml
    - ../env/{{ .cluster.provider }}/default.yaml
    - ../env/{{ .cluster.provider }}/{{ .cluster.name }}.yaml
    - ../core.yaml
